{
    "version": 2,
    "builds": [      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "backend/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      },
      {
        "src": "/analyze",
        "dest": "backend/app.py"
      }
    ]
  }